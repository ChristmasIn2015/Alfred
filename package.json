{
  "name": "yjy-ui",
  "version": "0.0.1",
  "description": "only used by * yjy CMS PC * yjy CMS mobile",
  "main": "main.js",
  "scripts": {
    "build": "webpack main.js --config webpack.config.js",
    "build2": "webpack main.js --config webpack.config.js && node ../setUI.js"
  },
  "author": "wqao",
  "license": "ISC",
  "dependencies": {
    "postcss-px-to-viewport": "^1.1.1",
    "vue-awesome-swiper": "^4.1.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "unitToConvert": "PX",
        "viewportWidth": 375,
        "viewportHeight": 667,
        "unitPrecision": 3,
        "viewportUnit": "vw",
        "selectorBlackList": [
          ".ignore",
          ".hairlines"
        ],
        "minPixelValue": 1,
        "mediaQuery": false
      }
    }
  }
}
